.windowover2.click{
    -webkit-animation:windowover2 1s ;   -moz-animation:windowover2 1s  ;
    -webkit-backface-visibility:visible;     -moz-backface-visibility:visible;

}

@-webkit-keyframes windowover2{  0%{-webkit-transform:perspective(500px) rotateY(180deg);}  100%{-webkit-transform:perspective(500px) rotateY(360deg);}
}
@-moz-keyframes windowover2{  0%{-moz-transform:perspective(500px) rotateY(180deg);}  100%{-moz-transform:perspective(500px) rotateY(360deg);}
}

