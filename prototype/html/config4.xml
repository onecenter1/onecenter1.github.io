                <items>
                    <item><text><![CDATA[10个]]></text></item>
                </items>
            </legend>
            <chart_background enabled="False"/>
        </chart_settings>

      <data_plot_settings enable_3d_mode="false">

        <pie_series radius="120"  inner_radius="70"  start_angle="280" style="Aqua"   >

            <pie_style>
                <border enabled="False"/>
            </pie_style>

            <!--鼠标移上某一块的时候显示内容-->
          <!--<tooltip_settings enabled="true">-->
            <!--<position anchor="CenterTop" padding="10"/>-->
            <!--<format>${%Value}{scale:(1000)(1000)(1000)|( th)( mil)( bil),numDecimals:2}</format>-->
          <!--</tooltip_settings>-->
            <!--<position anchor="Center" valign="Center" halign="Center" padding="0"/>-->
           <!--设置每一块是否用线条连接起来-->
          <label_settings enabled="true" mode="Outside" multi_line_align="Center"  >
                        <font size="11" color="White">
                          <effects>
                            <drop_shadow enabled="true" distance="1" opacity="0.9" blur_x="1" blur_y="1"/>
                          </effects>
                        </font>
              <!--移上文字是否线条也选中-->
              <states>
                  <hover>
                      <font underline="true" />
                  </hover>
              </states>
            <format>{%YPercentOfSeries}{numDecimals:0}%</format>
              <!--<font size="14" bold="true" color="DarkColor(%Color)"/>-->
          </label_settings>

            <!--线条的颜色-->
            <connector color="%Color" opacity="0.5" />
          <!--<marker_settings enabled="true">-->
            <!--<marker type="None" />-->
            <!--<states>-->
              <!--<hover>-->
                <!--<marker type="Circle" anchor="CenterTop" />-->
              <!--</hover>-->
            <!--</states>-->
          <!--</marker_settings>-->

        </pie_series>
      </data_plot_settings>

        <styles>
            <pie_style name="ExplodedStyle">

                <effects enabled="True">
                    <drop_shadow enabled="True" distance="2" color="#C23716" opacity="1"/>
                    <inner_shadow enabled="True"  distance="2" color="#C23716" angle="180" opacity="0.5"/>
                    <glow enabled="True" distance="3" color="#C23716"/>
                </effects>
                <fill type="Gradient">
                    <gradient angle="0" type="Radial">
                        <key position="0" color="#E86843" />
                        <key position="1" color="#E7613C" />
                    </gradient>
                </fill>
                <border enabled="False"/>
            </pie_style>
        </styles>


